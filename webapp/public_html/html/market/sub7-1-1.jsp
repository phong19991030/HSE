	<script>
		//main에만 들어감
		$(function(){
			$('body').addClass('main');
			
		 //visualSlider
		  $('.visual-wrap ul > li .visual-txt > strong').textillate({
		    autoStart: false,
		    in: {
		      effect: 'flipInY'
		    },
		    out: {
		      effect: 'flipOutY'
		    }
		  });
		  var pauseTime = 10000;
		  var timeoutId;
		  var visualSlider = $(".v-lst").bxSlider({

		    auto: 'true',
		    pause: '8000',
		    speed: '8000',
		    mode: 'fade',
		    controls: true,
		    pager: false,
		    autoControls: false,
		    infiniteLoop: true,
		    onSliderLoad: function(currentIndex) {
		      $('.visual-wrap .slide-counter .current-index').text('0' + (currentIndex + 1));
		      $(".v-lst li:not([class='bx-clone'])").eq(0).addClass('active');
		      $(".v-lst li:not([class='bx-clone'])").eq(0).find('.visual-txt > strong').textillate('in');

		    },
		    onSlideAfter: function(currentSlide, totalSlides, currentSlideHtmlObject) {
		      if (currentSlideHtmlObject === visualSlider.getSlideCount() - 1) {
		        setTimeout(function() {
		          visualSlider.goToSlide(0);
		          visualSlider.startAuto();
		        }, pauseTime);
		      } else {
		        clearTimeout(timeoutId);
		        visualSlider.startAuto();
		      }
		      currentSlide.addClass('active');
		    },
		    onSlideBefore: function($slideElement, oldIndex, newIndex) {
		      $('.visual-wrap .slide-counter .current-index').text('0' + (newIndex + 1));
		      $('.v-lst').find('li').removeClass('active');
		      $('.v-lst').find('li > .visual-txt > strong').textillate('in');
		    },
		  });
		  $(".visual-wrap .slide-counter").append('0' + visualSlider.getSlideCount());
		});
	</script>
	
